public class IntCount {
	
	public static void main(String[] args) {
		Number1(100);	
		Number2(100);
		Number3(100);
	}
	
	static void Number1(int n) {
		for(int i = 0; i < n; i++)
			System.out.println(i + 1);
		
	}
	
	static void Number2(int n) {
		for(int i = 0; i < n; i++) {
			System.out.println(i + 1);
			if(i == 99)
				break;
		}
	}
	
	static void Number3(int n) {
		for(int i = 0; i < n; i++) {
			System.out.println(i + 1);
			if(i == 99)
				return;
		}
	}
	
}
